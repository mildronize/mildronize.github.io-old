  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "run-s uuid develop",
    "serve": "gatsby serve",
    "build": "run-s pageview build:only",
    "build:only": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:styles": "stylelint src/**/*.css",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "uuid": "node ./scripts/build/inject-uuid.js",
    "uuid:build": "tsc --project ./scripts/tsconfig.json",
    "uuid:build-if-not-exist": "path-exists ./scripts/build/inject-uuid.js || npm run uuid:build",
    "postinstall": "run-p husky uuid:build-if-not-exist",
    "husky": "husky install",
    "pageview": "ts-node ./scripts/ga-pageview.ts"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
